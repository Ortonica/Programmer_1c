	Запрос = СтрЗаменить(Запрос, "#CreationDate", XMLДата(ТекущаяДата()));
	Запрос = СтрЗаменить(Запрос, "#Deadline", XMLДата(ПараметрыЗадачи.ВремяКонца));
	Запрос = СтрЗаменить(Запрос, "#Title", ПараметрыЗадачи.Заголовок);
	Запрос = СтрЗаменить(Запрос, "#Description", ПараметрыЗадачи.Описание);
	Запрос = СтрЗаменить(Запрос, "#Address", ПараметрыЗадачи.Адрес);
	//Запрос = СтрЗаменить(Запрос, "#Longitude","39.222058");  
	//Запрос = СтрЗаменить(Запрос, "#Latitude", "51.668056");
	//Запрос = СтрЗаменить(Запрос, "#ExternalID", "test doc number");
	Запрос = СтрЗаменить(Запрос, "#ClientName", ПараметрыЗадачи.Клиент);
	Запрос = СтрЗаменить(Запрос, "#ClientPhone", ПараметрыЗадачи.Телефон);
	Запрос = СтрЗаменить(Запрос, "#Priority", ПараметрыЗадачи.Приоритет);	
	Запрос = СтрЗаменить(Запрос, "#StartDate", XMLДата(ПараметрыЗадачи.ВремяНачала));
	Запрос = СтрЗаменить(Запрос, "#Status", 0);
	Запрос = СтрЗаменить(Запрос, "#SecondAddress","");
	Запрос = СтрЗаменить(Запрос, "#SecondLongitude",0);
	Запрос = СтрЗаменить(Запрос, "#SecondLatitude",0);
	Запрос = СтрЗаменить(Запрос, "#ObjectID","-1");
	Запрос = СтрЗаменить(Запрос, "#CommentPolicy",0);
	Запрос = СтрЗаменить(Запрос, "#ServiceTime",0);
	Запрос = СтрЗаменить(Запрос, "#StartInterval",0);
	
	ИмяВременногоФайла = ПолучитьИмяВременногоФайла(".txt");
	ВременныйФайл = Новый ЗаписьТекста(ИмяВременногоФайла, КодировкаТекста.UTF8);
	ВременныйФайл.Записать(Запрос);
	ВременныйФайл.Закрыть();
	Чтение = Новый ЧтениеТекста(ИмяВременногоФайла);
	Запрос = Чтение.Прочитать();
	УдалитьФайлы(ВременныйФайл);
	
	MSXML = Новый COMОбъект("MSXML2.XMLHTTP");
	MSXML.open("POST", "https://b2b.locator.ru/mts/ws/me_b2b_54/task_api.asmx", 0);
	MSXML.setRequestHeader("Content-Type", "text/xml; charset=utf-8");
	MSXML.setRequestHeader("Content-Length", СтрДлина(Запрос));
	MSXML.setRequestHeader("SOAPAction", "http://locator.ru/LBS/SaveTask");
	MSXML.send(Запрос);
	ОтветПолучен = Ложь;
	Для НомерЦикла = 1 по 100 Цикл
		Если MSXML.readyState >3 Тогда
			ОтветПолучен = Истина;
			Прервать;
		КонецЕсли;      
	КонецЦикла;  
	
	Если не ОтветПолучен Тогда
		Возврат неопределено;
	КонецЕсли;	
	ТекстОтвета = MSXML.responseXML.xml;
	Ответ = Новый Структура("Статус, IdЗадачи");
	Если MSXML.status <> 200 Тогда 
		Ответ.Статус = MSXML.status;
		Ответ.IdЗадачи = -1;
		Возврат Ответ;		
	КонецЕсли;	
	ТекстОтвета = MSXML.responseText;
	// выделяем id задачи
	Чтение = Новый ЧтениеXML;
	Чтение.УстановитьСтроку(ТекстОтвета);
	Пока Чтение.Прочитать() Цикл
		Если Чтение.Имя = "SaveTaskResult" Тогда
			Чтение.Прочитать();
			IdЗадачи = Чтение.Значение;
			Ответ.Статус = MSXML.status;
			Ответ.IdЗадачи = IdЗадачи;
			Возврат Ответ;	
		КонецЕсли;	
	КонецЦикла;	
	Ответ.Статус = MSXML.status;
	Ответ.IdЗадачи = -1;
	Возврат Ответ;	
КонецФункции	

